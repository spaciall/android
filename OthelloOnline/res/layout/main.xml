<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background = "@drawable/background"
    >
	
	<ImageView android:id="@+id/IVTitle" android:src="@drawable/title" android:paddingTop = "55dip" android:paddingBottom = "45dip" android:layout_width="wrap_content" android:layout_height="wrap_content">
	</ImageView>
	
	<ImageView android:id="@+id/IVStart" android:background="@drawable/button_start" android:layout_gravity = "center" android:layout_width="wrap_content" android:layout_height="wrap_content">
	</ImageView>

	<ImageView android:id="@+id/IVNetwork" android:background="@drawable/button_network" android:layout_gravity = "center" android:layout_width="wrap_content" android:layout_height="wrap_content">
	</ImageView>

	<ImageView android:id="@+id/IVOption" android:background="@drawable/button_option" android:layout_gravity = "center" android:layout_width="wrap_content" android:layout_height="wrap_content">
	</ImageView>

	<ImageView android:id="@+id/IVManual" android:background="@drawable/button_manual"  android:layout_gravity = "center" android:layout_width="wrap_content" android:layout_height="wrap_content">
	</ImageView>


</LinearLayout>
